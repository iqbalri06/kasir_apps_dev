{
    "builds": [{
        "src": "simple_shop/vercel_handler.py",
        "use": "@vercel/python",
        "config": {
            "runtime": "python3.9",
            "maxDuration": 60
        }
    }, {
        "src": "simple_shop/static/**",
        "use": "@vercel/static"
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "simple_shop/vercel_handler.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "simple_shop.settings"
    }
}
