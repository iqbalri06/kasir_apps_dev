{
    "builds": [{
        "src": "simple_shop/vercel_handler.py",
        "use": "@vercel/python"
    }, {
        "src": "simple_shop/static/**",
        "use": "@vercel/static"
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/simple_shop/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "simple_shop/vercel_handler.py"
        }
    ]
}
